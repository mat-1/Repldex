{
	"name": "repldex",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"dev:host": "svelte-kit dev --host",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"preview:host": "svelte-kit preview --host",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"register-commands": "node --experimental-specifier-resolution=node --loader ts-node/esm src/lib/discord/scripts/registerCommands.ts"
	},
	"devDependencies": {
		"@sveltejs/kit": "^1.0.0-next.186",
		"@types/cookie": "^0.4.1",
		"@types/jsonwebtoken": "^8.5.5",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"node-fetch": "^3.0.0",
		"postcss": "^8.3.9",
		"postcss-load-config": "^3.1.0",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.7",
		"svelte-preprocess": "^4.9.4",
		"ts-node": "^10.4.0",
		"tslib": "^2.3.1",
