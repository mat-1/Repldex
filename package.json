		"vite-plugin-html": "^2.1.1"
	},
	"type": "module",
	"dependencies": {
		"@lukeed/uuid": "^2.0.0",
		"@sveltejs/adapter-auto": "^1.0.0-next.11",
		"cookie": "^0.4.1",
		"discord-api-types": "^0.24.0",
		"discord-interactions": "^2.4.1",
		"dotenv": "^10.0.0",
		"jsonwebtoken": "^8.5.1",
		"mongodb": "^4.1.3"
	},
	"engines": {
		"node": ">=14.x"
	}
}
